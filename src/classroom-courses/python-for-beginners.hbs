<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{title}}</title>
    {{> meta}}
    {{> head}}
    {{> courseSchema}}
</head>
<body class="scroll-assist">
{{> navbar}}
<div class="main-container">
    {{> header}}
    {{> overview}}
    {{> coursehighlights}}
	{{> coursecontents}}
    {{> team team=mentors}}
	{{> courseenroll}}
	{{> courseschedule}}
    <section>
                                    <div class="text-center">
                                        <h3>REGISTER NOW</h3></div>
                                    <div class="container">
                                        <div class="course-tabs">
                                            <ul class="nav nav-tabs" role="tablist">
                                                <li role="presentation" class="wide-tab active" id="tab-delhi"><a href="#delhi" aria-controls="home" role="tab" data-toggle="tab" aria-expanded="true">Pitampura</a></li>
                                                <li role="presentation" class="wide-tab" id="tab-dwarka"><a href="#dwarka" aria-controls="home" role="tab" data-toggle="tab" aria-expanded="true">Dwarka</a></li>
                                                <li role="presentation" class="wide-tab" id="tab-dehradun"><a href="#dehradun" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="false">Dehradun (50% inaugural discount)</a></li>
                                            </ul>
                                            <div class="tab-content">
                                                <div role="tabpanel" class="tab-pane active" id="delhi">
                                                    <div class="widget-container" {{#if frame-id-pitampura}}data-widget-id='{{frame-id-pitampura}}'{{/if}}>
                                                            <iframe class="payment-widget" {{#if frame-id-pitampura}}id="{{frame-id-pitampura}}"{{/if}} frameborder="0" src="{{{registerLinkPitampura}}}"></iframe>
                                                    </div>
                                                </div>
                                                <div role="tabpanel" class="tab-pane" id="dwarka">
                                                    <div class="widget-container" {{#if frame-id-dwarka}}data-widget-id='{{frame-id-dwarka}}'{{/if}}>
                                                            <iframe class="payment-widget" {{#if frame-id-dwarka}}id="{{frame-id-dwarka}}"{{/if}} frameborder="0" src="{{{registerLinkDwarka}}}"></iframe>
                                                    </div>
                                                </div>
                                                <div role="tabpanel" class="tab-pane" id="dehradun">
													<div class="widget-container" {{#if frame-id-dehradun}}data-widget-id='{{frame-id-dehradun}}'{{/if}}>
															<iframe class="payment-widget" {{#if frame-id-dehradun}}id="{{frame-id-dehradun}}"{{/if}} frameborder="0" src="{{{registerLinkDehradun}}}"></iframe>
													</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
    {{> coursefaq}}
    {{> map}}
    {{> footer}}
</div>
{{> scripts}}
</body>
</html>
