<!--overview-->
{{#with overview}}
    <section>
        <div class="container">
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1 text-center">
                    <h2 class="uppercase color-primary mb40 mb-xs-24">Overview</h2>
                    <p class="lead">{{{content}}}</p><br>
					<a class="btn btn-lg btn-filled" {{#if buttonDisabled}}disabled{{/if}} href="{{#if buttonLink}}{{{buttonLink}}}{{else}}#register{{/if}}">{{#if buttonText}}{{{buttonText}}}{{else}}Register Now{{/if}}</a>
                </div>
            </div>
        </div>
    </section>
{{/with}}
<!--endoverview-->
